    "mongoose-paginate-v2": "^1.3.13",
    "mongoose-plugin-autoinc": "^1.1.9",
    "mongoose-timestamp": "^0.6.0",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.10.0",
